{
  "extends": "@grafana/tsconfig",
  "compilerOptions": {
    "allowJs": true,
    "importHelpers": false,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "module": "preserve",
    "moduleDetection": "force",
    "noEmit": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "target": "es2022",
    "verbatimModuleSyntax": true
  },
  "include": ["src"]
}
