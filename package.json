{
  "name": "@grafana/plugin-ui",
  "version": "0.12.1",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/grafana/plugin-ui.git"
  },
  "author": "Grafana Labs",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && rollup -c rollup.config.ts --configPlugin esbuild",
    "dev": "tsc -p ./tsconfig.build.json && rollup -c rollup.config.ts --configPlugin esbuild -w",
    "clean": "rm -rf ./dist ./compiled",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,md,mdx,yml,yaml,json,scss,css}\"",
    "storybook": "storybook dev",
    "storybook:build": "storybook build",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "typecheck": "tsc",
    "verify-types": "attw --pack ."
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@hello-pangea/dnd": "^17.0.0",
    "@react-awesome-query-builder/ui": "^6.6.4",
    "@types/prismjs": "^1.26.4",
    "lodash": "^4.17.21",
    "prismjs": "^1.30.0",
    "react-calendar": "^4.8.0",
    "react-popper-tooltip": "^4.4.2",
    "react-use": "^17.3.1",
    "react-virtualized-auto-sizer": "^1.0.6",
    "sql-formatter-plus": "^1.3.6",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@grafana/data": "^10.4.0",
    "@grafana/e2e-selectors": "^10.4.0",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/runtime": "^10.4.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@grafana/ui": "^10.4.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@storybook/addon-docs": "9.1.17",
    "@storybook/addon-links": "9.1.17",
    "@storybook/addon-webpack5-compiler-swc": "4.0.2",
    "@storybook/react-webpack5": "9.1.17",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@swc/core": "1.15.11",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "15.0.2",
    "@testing-library/user-event": "14.5.2",
    "@types/chance": "^1.1.0",
    "@types/lodash": "^4.14.194",
    "@types/memoize-one": "^5.1.2",
    "@types/react": "18.3.18",
    "@types/react-calendar": "^3.1.2",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "chance": "^1.1.7",
    "copyfiles": "^2.4.1",
    "cspell": "^8.14.4",
    "esbuild": "^0.25.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "lefthook": "^1.7.18",
    "mockdate": "^3.0.2",
    "prettier": "^3.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select-event": "^5.5.1",
    "rollup": "^4.32.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-node-externals": "^8.0.0",
    "rxjs": "^7.8.1",
    "storybook": "9.1.17",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@grafana/data": ">=10.4.0",
    "@grafana/e2e-selectors": ">=10.4.0",
    "@grafana/runtime": ">=10.4.0",
    "@grafana/ui": ">=10.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1"
  },
  "files": [
    "dist"
  ],
  "resolutions": {
    "prismjs": "^1.30.0",
    "uplot": "^1.6.31",
    "glob": "10.5.0",
    "tar": "^7.5.7",
    "@babel/helpers": "^7.26.10",
    "brace-expansion": "^2.0.2",
    "diff": "^4.0.4",
    "js-yaml": "^4.1.1"
  },
  "packageManager": "yarn@4.6.0"
}
